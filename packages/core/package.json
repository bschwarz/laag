{
  "name": "@laag/core",
  "version": "2.0.0",
  "description": "Core utilities and base classes for the laag library collection",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "bun run ../../scripts/build-package.ts",
    "build:watch": "bun run ../../scripts/build-package.ts --watch",
    "build:clean": "rm -rf dist",
    "clean": "rm -rf dist",
    "dev": "bun run build:watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "api",
    "specification",
    "openapi",
    "swagger",
    "raml",
    "typescript",
    "utilities"
  ],
  "author": "laag contributors",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
