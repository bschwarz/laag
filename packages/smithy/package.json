{
  "name": "@laag/smithy",
  "version": "1.0.1",
  "description": "TypeScript library for interfacing with AWS Smithy models",
  "keywords": [
    "smithy",
    "aws",
    "api",
    "idl",
    "interface-definition",
    "design",
    "typescript",
    "code-generation"
  ],
  "author": "Brett Schwarz <brett.schwarz@gmail.com>",
  "homepage": "https://github.com/bschwarz/laag/tree/main/packages/smithy",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "browser": "./dist/browser/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "browser": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/browser/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bschwarz/laag.git",
    "directory": "packages/smithy"
  },
  "scripts": {
    "build": "bun run ../../scripts/build-package.ts",
    "build:watch": "bun run ../../scripts/build-package.ts --watch",
    "build:clean": "rm -rf dist",
    "clean": "rm -rf dist",
    "dev": "bun run build:watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "type-check": "tsc --noEmit --skipLibCheck",
    "lint": "eslint src/**/*.ts __tests__/**/*.ts",
    "lint:fix": "eslint src/**/*.ts __tests__/**/*.ts --fix",
    "docs": "typedoc",
    "docs:clean": "rm -rf docs/api",
    "docs:serve": "bun run docs && cd docs/api && python3 -m http.server 8080",
    "prepublishOnly": "bun run build && bun run test && bun run type-check"
  },
  "bugs": {
    "url": "https://github.com/bschwarz/laag/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@laag/core": "workspace:*",
    "typescript": ">=4.5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@laag/core": "workspace:*",
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "path": "/Users/brettschwarz/repos/laag/packages/smithy",
  "packageJsonPath": "/Users/brettschwarz/repos/laag/packages/smithy/package.json",
  "directoryName": "smithy"
}
